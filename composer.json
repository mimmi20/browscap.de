{
  "name": "mimmi20/browscap.de",
  "type": "project",
  "description": "experimental page for the browscap project built with Zend Expressive",
  "homepage": "https://github.com/mimmi20/browscap.de",
  "license": "MIT",
  "require": {
    "php": "^7.1",
    "ext-json": "*",
    "beberlei/assert": "^3.2.7",
    "browscap/browscap": "^6.0.37",
    "browscap/browscap-php": "^4.2.2 || ^5.0.0",
    "laminas/laminas-cache": "^2.9.0",
    "laminas/laminas-component-installer": "~2.1.2",
    "laminas/laminas-config-aggregator": "~1.2.0",
    "laminas/laminas-filter": "~2.9.3",
    "laminas/laminas-form": "~2.14.3",
    "laminas/laminas-hydrator": "~2.4.2 || ~3.0.2",
    "laminas/laminas-i18n": "~2.10.1",
    "laminas/laminas-inputfilter": "~2.10.1",
    "laminas/laminas-serializer": "~2.9.1",
    "laminas/laminas-servicemanager": "~3.4.0",
    "laminas/laminas-session": "~2.9.2",
    "laminas/laminas-stdlib": "~3.2.1",
    "laminas/laminas-validator": "~2.13.1",
    "mezzio/mezzio": "~3.2.1",
    "mezzio/mezzio-csrf": "~1.0.1",
    "mezzio/mezzio-helpers": "~5.3.0",
    "mezzio/mezzio-laminasrouter": "~3.0.1",
    "mezzio/mezzio-session-ext": "~1.7.0",
    "mezzio/mezzio-twigrenderer": "^2.5.0",
    "mimmi20/ua-generic-request": "^5.0.5",
    "monolog/monolog": "^1.25.3 || ^2.0.2",
    "psr/http-message": "^1.0.1",
    "psr/http-server-middleware": "^1.0.1",
    "roave/security-advisories": "dev-master",
    "stickee/cache": "dev-master",
    "symfony/twig-bridge": "^4.4.5 || ^5.0.5",
    "thadafinser/package-info": "^1.0"
  },
  "require-dev": {
    "ergebnis/composer-normalize": "^2.2.4",
    "ergebnis/phpstan-rules": "^0.14.3",
    "filp/whoops": "^2.7.1",
    "friendsofphp/php-cs-fixer": "^2.16.1",
    "jangregor/phpstan-prophecy": "^0.6.2",
    "laminas/laminas-development-mode": "~3.2.0",
    "pepakriz/phpstan-exception-rules": "^0.10.1",
    "phpspec/prophecy": "^1.10.3",
    "phpstan/phpstan": "^0.12.14",
    "phpstan/phpstan-beberlei-assert": "^0.12.2",
    "phpstan/phpstan-deprecation-rules": "^0.12.2",
    "phpstan/phpstan-phpunit": "^0.12.6",
    "phpstan/phpstan-strict-rules": "^0.12.2",
    "phpunit/phpunit": "^7.5.20 || ^8.5.2 || ~9.0.1",
    "squizlabs/php_codesniffer": "^3.5.4"
  },
  "config": {
    "platform": {
      "php": "7.1.3"
    },
    "preferred-install": "dist",
    "sort-packages": true
  },
  "autoload": {
    "psr-4": {
      "App\\": "src/App/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "AppTest\\": "test/AppTest/"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "scripts": {
    "post-install-cmd": [
      "@update-browscap-cache"
    ],
    "post-update-cmd": [
      "@update-browscap-cache"
    ],
    "post-create-project-cmd": [
      "@development-enable"
    ],
    "check": [
      "@cs-check",
      "@test"
    ],
    "clear-config-cache": "php bin/clear-config-cache.php",
    "cs-check": "phpcs",
    "cs-fix": "phpcbf",
    "development-disable": "zf-development-mode disable",
    "development-enable": "zf-development-mode enable",
    "development-status": "zf-development-mode status",
    "serve": "php -S 0.0.0.0:8080 -t public public/index.php",
    "test": "phpunit --colors=always",
    "test-coverage": "phpunit --colors=always --coverage-clover clover.xml",
    "update-browscap-cache": "php -d memory_limit=-1 bin/update-browscap.php"
  }
}
