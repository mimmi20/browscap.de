{
  "name": "mimmi20/browscap.de",
  "type": "project",
  "description": "experimental page for the browscap project built with Zend Expressive",
  "homepage": "https://github.com/mimmi20/browscap.de",
  "license": "MIT",
  "require": {
    "php": "^7.1",
    "ext-json": "*",
    "beberlei/assert": "^2.9 || ^3.0",
    "browscap/browscap": "^6.0.31",
    "browscap/browscap-php": "^4.1",
    "mimmi20/ua-generic-request": "^4.0.1",
    "monolog/monolog": "^1.24",
    "psr/http-message": "^1.0.1",
    "psr/http-server-middleware": "^1.0.0",
    "roave/security-advisories": "dev-master",
    "stickee/cache": "dev-master",
    "symfony/twig-bridge": "^4.1",
    "thadafinser/package-info": "^1.0",
    "zendframework/zend-cache": "^2.8.0",
    "zendframework/zend-component-installer": "~2.1.1",
    "zendframework/zend-config-aggregator": "~1.1.1",
    "zendframework/zend-expressive": "~3.2.1",
    "zendframework/zend-expressive-csrf": "~1.0.0",
    "zendframework/zend-expressive-helpers": "~5.1.2",
    "zendframework/zend-expressive-session-ext": "~1.4.0",
    "zendframework/zend-expressive-twigrenderer": "~2.3.0",
    "zendframework/zend-expressive-zendrouter": "~3.0.0",
    "zendframework/zend-filter": "~2.9.1",
    "zendframework/zend-form": "~2.14.0",
    "zendframework/zend-hydrator": "~2.4.0",
    "zendframework/zend-i18n": "~2.9.0",
    "zendframework/zend-inputfilter": "~2.10.0",
    "zendframework/zend-serializer": "~2.9.0",
    "zendframework/zend-servicemanager": "~3.4.0",
    "zendframework/zend-session": "~2.8.5",
    "zendframework/zend-stdlib": "~3.2.0",
    "zendframework/zend-validator": "~2.12.0"
  },
  "require-dev": {
    "localheinz/phpstan-rules": "^0.7.1",
    "filp/whoops": "^2.2.0",
    "friendsofphp/php-cs-fixer": "^2.13",
    "pepakriz/phpstan-exception-rules": "^0.3 || ^0.4",
    "phpstan/phpstan": "^0.10",
    "phpstan/phpstan-beberlei-assert": "^0.10",
    "phpstan/phpstan-deprecation-rules": "^0.10",
    "phpstan/phpstan-phpunit": "^0.10",
    "phpstan/phpstan-strict-rules": "^0.10",
    "phpunit/phpunit": "^7.5",
    "squizlabs/php_codesniffer": "^3.4",
    "zfcampus/zf-development-mode": "~3.2.0"
  },
  "config": {
    "optimize-autoloader": true,
    "preferred-install": "dist",
    "sort-packages": true
  },
  "autoload": {
    "psr-4": {
      "App\\": "src/App/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "AppTest\\": "test/AppTest/"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "scripts": {
    "post-install-cmd": [
      "@update-browscap-cache"
    ],
    "post-update-cmd": [
      "@update-browscap-cache"
    ],
    "post-create-project-cmd": [
      "@development-enable"
    ],
    "check": [
      "@cs-check",
      "@test"
    ],
    "clear-config-cache": "php bin/clear-config-cache.php",
    "cs-check": "phpcs",
    "cs-fix": "phpcbf",
    "development-disable": "zf-development-mode disable",
    "development-enable": "zf-development-mode enable",
    "development-status": "zf-development-mode status",
    "serve": "php -S 0.0.0.0:8080 -t public public/index.php",
    "test": "phpunit --colors=always",
    "test-coverage": "phpunit --colors=always --coverage-clover clover.xml",
    "update-browscap-cache": "php -d memory_limit=-1 bin/update-browscap.php"
  }
}
