{
    "name": "mimmi20/browscap.de",
    "description": "experimental page for the browscap project built with Zend Expressive",
    "type": "project",
    "homepage": "https://github.com/mimmi20/browscap.de",
    "license": "MIT",
    "config": {
        "sort-packages": true
    },
    "repositories": [
        {
            "type": "git",
            "url":  "https://github.com/mimmi20/cache.git"
        }
    ],
    "require": {
        "php": "^7.1",
        "psr/http-message": "^1.0.1",
        "browscap/browscap-php": "dev-master",
        "http-interop/http-middleware": "^0.4.1",
        "roave/security-advisories": "dev-master",
        "stickee/cache": "dev-master",
        "zendframework/zend-component-installer": "^1.0",
        "zendframework/zend-config-aggregator": "^1.0",
        "zendframework/zend-expressive": "^2.0.5",
        "zendframework/zend-expressive-csrf": "^0.1.1",
        "zendframework/zend-expressive-helpers": "^4.0",
        "zendframework/zend-expressive-session-ext": "^0.1.2",
        "zendframework/zend-expressive-twigrenderer": "^1.4",
        "zendframework/zend-expressive-zendrouter": "^2.0.1",
        "zendframework/zend-filter": "^2.7",
        "zendframework/zend-form": "^2.11",
        "zendframework/zend-i18n": "^2.7",
        "zendframework/zend-inputfilter": "^2.8",
        "zendframework/zend-serializer": "^2.8",
        "zendframework/zend-servicemanager": "^3.3",
        "zendframework/zend-session": "^2.8",
        "zendframework/zend-stdlib": "^3.1",
        "zendframework/zend-validator": "^2.10"
    },
    "require-dev": {
        "phpunit/phpunit": "^6.5",
        "squizlabs/php_codesniffer": "^3.2",
        "friendsofphp/php-cs-fixer": "^2.10",
        "phpstan/phpstan": "^0.9",
        "zfcampus/zf-development-mode": "^3.1",
        "filp/whoops": "^2.1.7"
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/App/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "AppTest\\": "test/AppTest/"
        }
    },
    "scripts": {
        "post-create-project-cmd": [
            "@development-enable"
        ],
        "development-disable": "zf-development-mode disable",
        "development-enable": "zf-development-mode enable",
        "development-status": "zf-development-mode status",
        "check": [
            "@cs-check",
            "@test"
        ],
        "clear-config-cache": "php bin/clear-config-cache.php",
        "cs-check": "phpcs",
        "cs-fix": "phpcbf",
        "serve": "php -S 0.0.0.0:8080 -t public public/index.php",
        "test": "phpunit --colors=always",
        "test-coverage": "phpunit --colors=always --coverage-clover clover.xml",
        "update-browscap-cache": "php -d memory_limit=-1 bin/update-browscap.php"
    }
}
